		"commandCenter.border": "#e7e7e799",
		"sash.hoverBorder": "#299ac0",
		"statusBar.background": "#207896",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#299ac0",
		"statusBarItem.remoteBackground": "#207896",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#207896",
